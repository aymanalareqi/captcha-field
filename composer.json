{
    "name": "alareqi/captcha-field",
    "description": "Filament Captcha Field",
    "keywords": [
        "alareqi",
        "laravel",
        "captcha-field"
    ],
    "homepage": "https://github.com/alareqi/captcha-field",
    "support": {
        "issues": "https://github.com/alareqi/captcha-field/issues",
        "source": "https://github.com/alareqi/captcha-field"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ayman Alareqi",
            "email": "ayman.a.alareqi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "mews/captcha": "^3.2",
        "illuminate/contracts": "^10.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0|^7.0",
        "orchestra/testbench": "^8.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Alareqi\\CaptchaField\\": "src/",
            "Alareqi\\CaptchaField\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alareqi\\CaptchaField\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alareqi\\CaptchaField\\CaptchaFieldServiceProvider"
            ],
            "aliases": {
                "CaptchaField": "Alareqi\\CaptchaField\\Facades\\CaptchaField"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}